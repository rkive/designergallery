---
title: Designer Gallery
---
#main-container
  %header
    = image_tag "header.jpg"
  .container

    = image_tag "top_channel_banner.jpg"
    #main-gallery
      #main-gallery-images.carousel.slide
        .carousel-inner
          .item.active
            = image_tag "gallery_1.jpg"
          .item
            %a(href="/celebrity.html")
              = image_tag "gallery_2.jpg"
          .item
            = image_tag "gallery_3.jpg"
        %a.left.carousel-control(href="#main-gallery-images" data-slide="prev") ‹
        %a.right.carousel-control(href="#main-gallery-images" data-slide="next") ›
    .profile-gallery
      %a#profile-link-1(href="/celebrity.html")
      %a#profile-link-2(href="/phedinkus.html")
      %a#profile-link-3(href="/50_cent.html")
      = image_tag "home_page.jpg"
      

  %footer.footer#footer
    = image_tag "footer.jpg"

